<%= form_with(model: @contact, local: true) do |f| %>



  <div class="title">
    <%= f.label :title %>
    <%= f.text_field :title, placeholder: "タイトルを入力してね" %>
  </div>

  <div class="body">
    <%= f.label :body %>
    <%= f.text_field :body, placeholder: "「ひとこと」を入力してね" %>
  </div>


  <div class="submit">
    <%= f.submit %>
  </div>

<% end %>
